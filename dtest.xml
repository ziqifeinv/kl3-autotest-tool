<?xml version="1.0" encoding="UTF-8"?>
<!-- 这是kl3-test-tool软件对应的dtest配置文件，主要功能如下：
    1.枚举dtest程序，name节点用于展示，要求和对应的dtest固件名称相同
    2.枚举dtest程序中存在的测试case，name用于展示，要求id和程序中case编号相同
    3.保存部分界面配置信息

    [note]:这一段注释会导致另存xml文件时错位，不过不影响功能
-->
<root_dtest>
    <debug_interface>true</debug_interface>
    <port id="0">
        <com>COM3</com>
        <baud>115200</baud>
        <data>8</data>
        <parity>NONE</parity>
        <stop>1.0</stop>
    </port>
    <port id="1">
        <com>COM4</com>
        <baud>9600</baud>
        <data>8</data>
        <parity>NONE</parity>
        <stop>1.0</stop>
    </port>
    <port id="2">
        <com></com>
        <baud>115200</baud>
        <data>8</data>
        <parity>NONE</parity>
        <stop>1.0</stop>
    </port>
    <port id="3">
        <com></com>
        <baud>9600</baud>
        <data>8</data>
        <parity>NONE</parity>
        <stop>1.0</stop>
    </port>
    <dir id="0">D:\workspace\vsc\python\ssh\dtest_file</dir>
    <dir id="1"/>
    <dtest id="000">
        <name>unit_test_smc</name>
        <note>用于测试smc（psram）的基础功能</note>
        <enable>true</enable>
        <slave_mode>false</slave_mode>
        <case id="00">
            <name>bus_test</name>
            <note>测试smc（cache）地址总线、数据总线、物理设备完整性</note>
            <enable>true</enable>
        </case>
        <case id="01">
            <name>cache_test</name>
            <note>测试smc cache读写功能</note>
            <enable>true</enable>
        </case>
        <case id="02">
            <name>api_test</name>
            <note>测试smc api读写功能</note>
            <enable>false</enable>
        </case>
    </dtest>
    <dtest id="001">
        <name>unit_test_sfc</name>
        <note>用于测试sfc基础功能</note>
        <enable>false</enable>
        <slave_mode>false</slave_mode>
        <case id="00">
            <name>case_1</name>
            <note>默认测试</note>
            <enable>true</enable>
        </case>
    </dtest>
    <rule id="0">
        <keyword>START</keyword>
        <color>#000000</color>
        <enable>true</enable>
    </rule>
    <rule id="1">
        <keyword>REBOOT</keyword>
